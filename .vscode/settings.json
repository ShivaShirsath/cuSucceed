{
	"security.workspace.trust.enabled": false,
	"terminal.integrated.gpuAcceleration": "off",
	"workbench.colorTheme": "Darcula",
	"workbench.iconTheme": "material-icon-theme",
	"window.commandCenter": true,
	"editor.fontSize": 16,
	"editor.fontFamily": "Courier New",
	"files.associations": {
		"*.js": "javascriptreact",
		"*.html": "html"
	},
	"[css]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"editor.tabSize": 2,
	"editor.bracketPairColorization.independentColorPoolPerBracketType": true,
	"editor.experimental.pasteActions.enabled": true,
	"editor.foldingImportsByDefault": true,
	"editor.mouseWheelZoom": true,
	"editor.stickyScroll.enabled": true,
	"editor.tokenColorCustomizations": {},
	"editor.stickyTabStops": true,
	"editor.stickyScroll.maxLineCount": 10,
	"editor.tabCompletion": "on",
	"editor.cursorSmoothCaretAnimation": true,
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"editor.formatOnType": true,
	"editor.minimap.autohide": true,
	"files.simpleDialog.enable": true,
	"workbench.preferredDarkColorTheme": "Darcula",
	"workbench.preferredHighContrastLightColorTheme": "Darcula",
	"workbench.preferredLightColorTheme": "Darcula",
	"workbench.editor.enablePreviewFromQuickOpen": true,
	"window.openFoldersInNewWindow": "off",
	"search.showLineNumbers": true,
	"scm.diffDecorationsGutterPattern": {
		"added": true
	},
	"terminal.integrated.allowMnemonics": true,
	"extensions.ignoreRecommendations": true,
	"terminal.integrated.cursorBlinking": true,
	"terminal.integrated.defaultProfile.linux": "fish",
	"terminal.integrated.smoothScrolling": true,
	"remote.localPortHost": "allInterfaces",
	"notebook.lineNumbers": "on",
	"emmet.showSuggestionsAsSnippets": true,
	"errorLens.gutterIconsEnabled": true,
	"errorLens.margin": "2ch",
	"errorLens.onSave": true,
	"errorLens.scrollbarHackEnabled": true,
	"errorLens.statusBarColorsEnabled": true,
	"errorLens.statusBarIconsEnabled": true,
	"errorLens.statusBarMessageEnabled": true,
	"git.enableSmartCommit": true,
	"git.mergeEditor": true,
	"git.terminalGitEditor": true,
	"html.format.indentHandlebars": true,
	"html.format.indentInnerHtml": true,
	"html.format.templating": true,
	"markdown.preview.lineHeight": 1,
	"markdown.preview.openMarkdownLinks": "inEditor",
	"markdown.preview.scrollEditorWithPreview": false,
	"markdown.preview.scrollPreviewWithEditor": false,
	"markdown.preview.typographer": true,
	"markdown.validate.enabled": true,
	"material-icon-theme.activeIconPack": "react",
	"material-icon-theme.showUpdateMessage": true,
	"merge-conflict.autoNavigateNextConflict.enabled": true,
	"npm.enableRunFromFolder": true,
	"npm.packageManager": "npm",
	"prettier.bracketSameLine": true,
	"prettier.enableDebugLogs": true,
	"prettier.trailingComma": "all",
	"prettier.useTabs": true,
	"svg.completion.emmet": true,
	"svg.completion.showAdvanced": true,
	"svg.completion.showDeprecated": true,
	"svg.minify.addAttributesToSVGElement": true,
	"svg.minify.addClassesToSVGElement": true,
	"svg.preview.background": "dark-transparent",
	"svg.preview.showCrossLine": true,
	"svg.preview.showRuler": true,
	"javascript.inlayHints.enumMemberValues.enabled": true,
	"javascript.inlayHints.functionLikeReturnTypes.enabled": true,
	"javascript.inlayHints.parameterNames.enabled": "all",
	"javascript.inlayHints.parameterTypes.enabled": true,
	"javascript.inlayHints.propertyDeclarationTypes.enabled": true,
	"javascript.inlayHints.variableTypes.enabled": true,
	"typescript.inlayHints.enumMemberValues.enabled": true,
	"typescript.inlayHints.parameterTypes.enabled": true,
	"typescript.inlayHints.propertyDeclarationTypes.enabled": true,
	"typescript.inlayHints.variableTypes.enabled": true,
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"window.menuBarVisibility": "classic",
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"debug.terminal.clearBeforeReusing": true,
	"terminal.integrated.fontSize": 12,
	"code-runner.runInTerminal": true
}
